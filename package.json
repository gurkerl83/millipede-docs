{
  "name": "millipede-docs",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "build/*",
    "packages/**/*"
  ],
  "description": "A story about Project Millipedes purpose and what we are aiming at",
  "author": "Markus Gritsch",
  "license": "MIT",
  "keywords": [
    "story",
    "description"
  ],
  "homepage": "https://millipede.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-millipede/millipede-docs.git"
  },
  "bugs": {
    "url": "https://github.com/project-millipede/millipede-docs/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "next:dev": "next-remote-watch ./docs/src/pages",
    "next:build": "next build",
    "next:start": "next start",
    "app:build": "tsc",
    "setup:yarn:base": "yarn set version berry && yarn plugin import workspace-tools",
    "setup:yarn:ide": "yarn dlx @yarnpkg/pnpify --sdk vscode",
    "setup:yarn": "yarn setup:yarn:base && yarn setup:yarn:ide",
    "switch:yarn:linker:prod": "yarn config set nodeLinker node-modules && yarn",
    "switch:yarn:linker:dev": "yarn config set nodeLinker pnp && yarn",
    "dev": "NODE_ENV=development && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn next:dev",
    "start": "NODE_ENV=production && yarn clean && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn next:build && yarn next:start",
    "vercel-build": "NODE_ENV=production && yarn clean && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn run next:build",
    "test": "NODE_ENV=development jest --colors --watch",
    "lint": "yarn eslint './docs/src/**/*' './src/**/*' './pages/**/*' './packages/**/src/**/*'",
    "format:mdx": "prettier --parser mdx --print-width 80 --prose-wrap always --single-quote true --write \"docs/src/pages/**/*.mdx\"",
    "format:json": "prettier --parser json --print-width 80 --use-tabs --write \"locales/**/*.json\"",
    "sitemap": "node sitemap-generator"
  },
  "dependencies": {
    "@app/analytics": "workspace:packages/analytics",
    "@app/components": "workspace:packages/app/components",
    "@app/houx": "workspace:packages/houx",
    "@app/layout": "workspace:packages/app/layout",
    "@app/mdx-compile": "workspace:packages/mdx-compile",
    "@app/mdx-runtime": "workspace:packages/mdx-runtime",
    "@app/types": "workspace:packages/types",
    "@app/utils": "workspace:packages/utils",
    "@demonstrator/layout": "workspace:packages/demonstrator/layout",
    "@demonstrators-social/layout": "workspace:packages/demonstrators/social/layout",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@huse/effect-ref": "^1.0.3",
    "@huse/merged-ref": "^1.1.2",
    "@material-ui/core": "5.0.0-alpha.18",
    "@material-ui/icons": "5.0.0-alpha.15",
    "@material-ui/lab": "5.0.0-alpha.18",
    "@page/illustrations": "workspace:packages/illustrations",
    "@page/landing": "workspace:packages/page/landing",
    "@page/layout": "workspace:packages/page/layout",
    "@rest-hooks/normalizr": "^5.0.6",
    "@types/resize-observer-browser": "^0.1.5",
    "@vangware/window-open-promise": "^4.0.10",
    "ahooks": "^2.9.4",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.7",
    "element-resize-detector": "^1.2.1",
    "factory.ts": "^0.5.1",
    "fast-glob": "^3.2.4",
    "framer-motion": "^3.2.1",
    "gray-matter": "^4.0.2",
    "group-array": "^1.0.0",
    "immer": "^8.0.0",
    "jss": "^10.5.0",
    "lodash": "^4.17.20",
    "mdast-util-toc": "^5.1.0",
    "next": "10.0.6-canary.2",
    "next-google-fonts": "^1.2.1",
    "next-merge-props": "^0.4.0",
    "next-translate": "1.0.1",
    "query-string": "^6.13.8",
    "react": "17.0.1",
    "react-compound-timer": "^1.2.0",
    "react-device-detect": "^1.15.0",
    "react-dom": "17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-scroll": "^1.8.1",
    "reading-time-estimator": "1.3.3",
    "recoil": "facebookexperimental/Recoil.git#nightly",
    "remark-parse": "^9.0.0",
    "remark-react": "^8.0.0",
    "remark-slug": "^6.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "typesafe-actions": "^5.1.0",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3",
    "use-analytics": "^0.0.5",
    "use-query-params": "^1.1.9",
    "use-resize-observer": "^7.0.0",
    "uuid": "^8.3.2",
    "vfile": "^4.2.1",
    "victory": "^35.4.6",
    "windups": "^1.1.4"
  },
  "devDependencies": {
    "@types/draft-js": "^0.10.44",
    "@types/element-resize-detector": "^1.1.2",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/unist": "^2.0.3",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "next-remote-watch": "^0.3.0",
    "nextjs-sitemap-generator": "^1.3.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "webpack-merge": "5.7.3"
  },
  "resolutions": {
    "webpack": "5.15.0",
    "styled-components": "^5.2.1",
    "next-translate": "1.0.1",
    "next": "10.0.6-canary.2",
    "@material-ui/icons": "5.0.0-alpha.15",
    "@material-ui/core": "5.0.0-alpha.18",
    "@material-ui/lab": "5.0.0-alpha.18",
    "@material-ui/system": "5.0.0-alpha.18",
    "@material-ui/styled-engine": "5.0.0-alpha.18",
    "@material-ui/styles": "5.0.0-alpha.18",
    "@material-ui/types": "5.1.2",
    "@material-ui/unstyled": "5.0.0-alpha.18",
    "@material-ui/utils": "5.0.0-alpha.18"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./.next/",
      "./dist/",
      "./yarn/",
      "./node_modules/"
    ]
  }
}
