{
  "name": "millipede-docs",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "build/*",
    "packages/**/*"
  ],
  "description": "A story about Project Millipedes purpose and what we are aiming at",
  "author": "Markus Gritsch",
  "license": "MIT",
  "keywords": [
    "story",
    "description"
  ],
  "homepage": "https://millipede.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-millipede/millipede-docs.git"
  },
  "bugs": {
    "url": "https://github.com/project-millipede/millipede-docs/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "next:dev": "next-remote-watch ./docs/src/pages",
    "next:build": "next build --no-lint",
    "next:start": "next start",
    "app:build": "tsc",
    "setup:yarn:base": "yarn set version 3.0.0-rc.5 && yarn plugin import workspace-tools",
    "setup:yarn:ide": "yarn dlx @yarnpkg/pnpify --sdk vscode",
    "setup:yarn": "yarn setup:yarn:base && yarn setup:yarn:ide",
    "switch:yarn:linker:prod": "yarn config set nodeLinker node-modules && yarn",
    "switch:yarn:linker:dev": "yarn config set nodeLinker pnp && yarn",
    "dev": "NODE_ENV=development && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn next:dev",
    "start": "NODE_ENV=production && yarn clean && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn next:build && yarn next:start",
    "vercel-build": "NODE_ENV=production && yarn clean && yarn workspaces foreach -pt run clean && yarn workspace @app/webpack-build run compile && yarn workspace @app/mdx-runtime run compile && yarn workspace @app/mdx-compile run compile && yarn run app:build && yarn run next:build",
    "test": "NODE_ENV=development jest --colors --watch",
    "lint": "yarn eslint './docs/src/**/*' './src/**/*' './pages/**/*' './packages/**/src/**/*'",
    "format:mdx": "prettier --parser mdx --end-of-line lf --print-width 80 --prose-wrap always --single-quote true --write \"docs/src/pages/**/*.mdx\"",
    "format:json": "prettier --parser json -print-width 80 --tab-width 2 --write \"locales/**/*.json\"",
    "sitemap": "node sitemap-generator"
  },
  "dependencies": {
    "@app/analytics": "workspace:packages/analytics",
    "@app/components": "workspace:packages/app/components",
    "@app/houx": "workspace:packages/houx",
    "@app/layout": "workspace:packages/app/layout",
    "@app/mdx-compile": "workspace:packages/mdx-compile",
    "@app/mdx-runtime": "workspace:packages/mdx-runtime",
    "@app/types": "workspace:packages/types",
    "@app/utils": "workspace:packages/utils",
    "@artsy/fresnel": "^3.0.1",
    "@demonstrators-social/layout": "workspace:packages/demonstrators/social/layout",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@huse/effect-ref": "^1.1.1",
    "@huse/merged-ref": "^1.2.1",
    "@mui/icons-material": "^5.0.0",
    "@mui/lab": "^5.0.0-alpha.47",
    "@mui/material": "^5.0.0",
    "@page/illustrations": "workspace:packages/illustrations",
    "@page/landing": "workspace:packages/page/landing",
    "@page/layout": "workspace:packages/page/layout",
    "@project-millipede/windups": "^1.1.12",
    "@rest-hooks/normalizr": "^5.0.6",
    "@stefanprobst/next-svg": "^2.0.0",
    "@stefanprobst/remark-extract-toc": "^2.0.0",
    "@types/pdfjs-dist": "^2.7.4",
    "@types/resize-observer-browser": "^0.1.6",
    "@vangware/window-open-promise": "^4.2.4",
    "ahooks": "^2.10.9",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.24.0",
    "draft-js": "^0.11.7",
    "element-resize-detector": "^1.2.3",
    "esbuild": "^0.12.28",
    "factory.ts": "^0.5.2",
    "fast-glob": "^3.2.7",
    "framer-motion": "4.1.18-rc.26",
    "gray-matter": "^4.0.3",
    "group-array": "^1.0.0",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "mdast-util-toc": "^6.1.0",
    "mdx-bundler": "^6.0.1",
    "next": "11.1.3-canary.24",
    "next-merge-props": "^0.5.0",
    "next-translate": "1.1.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-compound-timer": "^1.2.0",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-full-screen": "^1.1.0",
    "react-intersection-observer": "^8.32.1",
    "react-pdf": "^5.3.2",
    "react-sizeme": "^3.0.2",
    "reading-time-estimator": "1.3.4",
    "recoil": "^0.4.1",
    "remark-parse": "^10.0.0",
    "remark-react": "^9.0.1",
    "remark-slug": "^7.0.0",
    "remark-stringify": "^10.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.1",
    "typesafe-actions": "^5.1.0",
    "unified": "^10.1.0",
    "unist-util-visit": "^2.0.3",
    "use-analytics": "^0.0.5",
    "use-async-effect": "^2.2.3",
    "use-query-params": "^1.2.3",
    "uuid": "^8.3.2",
    "vfile": "5.1.0",
    "victory": "^36.0.1"
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.5",
    "@types/element-resize-detector": "^1.1.3",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-pdf": "^5.0.6",
    "@types/styled-components": "^5.1.14",
    "@types/unist": "2.0.6",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-mdx": "^1.15.1",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.0",
    "next-remote-watch": "^1.0.0",
    "nextjs-sitemap-generator": "^1.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "webpack-merge": "5.8.0"
  },
  "resolutions": {
    "next": "11.1.3-canary.24",
    "@types/unist": "2.0.4",
    "mdast-util-toc": "5.1.0"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./.next/",
      "./dist/",
      "./yarn/",
      "./node_modules/"
    ]
  }
}
