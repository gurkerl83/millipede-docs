{
  "name": "millipede-docs",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "build/*",
    "packages/**/*"
  ],
  "description": "A story about Project Millipedes purpose and what we are aiming at",
  "author": "Markus Gritsch",
  "license": "MIT",
  "keywords": [
    "story",
    "description"
  ],
  "homepage": "https://millipede.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-millipede/millipede-docs.git"
  },
  "bugs": {
    "url": "https://github.com/project-millipede/millipede-docs/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf .next",
    "next:dev": "next-remote-watch ./docs/src/pages ./blog/src/pages ./locales/",
    "setup:yarn:base": "yarn set version 3.0.0-rc.5 && yarn plugin import workspace-tools",
    "setup:yarn:ide": "yarn dlx @yarnpkg/pnpify --sdk vscode",
    "setup:yarn": "yarn setup:yarn:base && yarn setup:yarn:ide",
    "switch:yarn:linker:prod": "yarn config set nodeLinker node-modules && yarn",
    "switch:yarn:linker:dev": "yarn config set nodeLinker pnp && yarn",
    "dev": "yarn clean && yarn run next:dev",
    "start": "yarn clean && next build && next start",
    "test": "jest --colors --watch",
    "lint": "yarn eslint './docs/src/lib/**/*' './packages/**/src/**/*' './pages/**/*'",
    "format:mdx": "prettier --parser mdx --end-of-line lf --print-width 80 --prose-wrap always --single-quote true --write \"docs/src/pages/**/*.mdx\"",
    "format:json": "prettier --parser json -print-width 80 --tab-width 2 --write \"locales/**/*.json\"",
    "sitemap": "node sitemap-generator"
  },
  "dependencies": {
    "@artsy/fresnel": "^3.4.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@huse/effect-ref": "^1.1.1",
    "@huse/merged-ref": "^1.2.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@project-millipede/windups": "^1.1.12",
    "@rest-hooks/normalizr": "^5.0.6",
    "@stefanprobst/next-svg": "4.2.1",
    "@stefanprobst/remark-extract-toc": "^2.1.0",
    "@types/pdfjs-dist": "^2.10.378",
    "@use-gesture/react": "^10.2.9",
    "@vangware/window-open-promise": "^5.0.3",
    "ahooks": "^3.1.13",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "draft-js": "^0.11.7",
    "element-resize-detector": "^1.2.4",
    "factory.ts": "^1.0.0",
    "fast-glob": "^3.2.11",
    "framer-motion": "^6.2.8",
    "gray-matter": "^4.0.3",
    "group-array": "^1.0.0",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "mdx-bundler": "^8.0.1",
    "motion": "^10.6.2",
    "nanoid": "^3.3.1",
    "next": "12.1.0",
    "next-merge-props": "^0.5.3",
    "next-translate": "1.3.5",
    "react": "18.0.0-rc.0",
    "react-compound-timer": "^1.2.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "18.0.0-rc.0",
    "react-full-screen": "^1.1.0",
    "react-intersection-observer": "^8.33.1",
    "react-pdf": "^5.7.1",
    "react-reconciler": "0.27.0-rc.0",
    "reading-time-estimator": "1.5.0",
    "recoil": "^0.6.1",
    "remark-parse": "^10.0.1",
    "remark-slug": "^7.0.1",
    "remark-stringify": "^10.0.2",
    "styled-components": "^5.3.3",
    "unified": "^10.1.2",
    "use-analytics": "^0.0.5",
    "use-async-effect": "^2.2.5",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "^8.3.2",
    "vfile": "^5.3.2",
    "victory": "^36.3.0"
  },
  "devDependencies": {
    "@types/draft-js": "^0.11.9",
    "@types/element-resize-detector": "^1.1.3",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-pdf": "^5.0.9",
    "@types/styled-components": "^5.1.24",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "next-remote-watch": "^1.0.0",
    "nextjs-sitemap-generator": "^1.3.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "webpack-merge": "5.8.0"
  },
  "resolutions": {
    "@stefanprobst/next-svg": "3.0.3",
    "react": "18.0.0-rc.0-next-fa816be7f-20220128",
    "react-dom": "18.0.0-rc.0-next-fa816be7f-20220128",
    "react-reconciler": "0.27.0-rc.0-next-fa816be7f-20220128",
    "recoil": "facebookexperimental/Recoil.git#nightly",
    "next": "12.1.1-canary.6"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./.next/",
      "./dist/",
      "./yarn/",
      "./node_modules/"
    ]
  }
}
