    "@mdx-js/react": "^1.6.21",
    "@rest-hooks/normalizr": "^5.0.6",
    "@vangware/window-open-promise": "^4.0.4",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.7",
    "element-resize-detector": "^1.2.1",
