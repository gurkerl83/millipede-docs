    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-merge": "^4.1.5",
