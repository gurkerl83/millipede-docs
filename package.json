    "webpack": "^5.4.0",
    "webpack-merge": "^5.4.0"
  },
  "resolutions": {
    "webpack": "^5.4.0"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./.next/",
      "./dist/",
      "./node_modules/"
    ]
  }
}
