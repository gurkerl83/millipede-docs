    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "loader-utils": "^2.0.0",
    "nextjs-sitemap-generator": "^1.1.3",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
