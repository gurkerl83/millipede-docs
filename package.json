{
  "name": "millipede-docs",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "build/*",
    "packages/**/*"
  ],
  "description": "A story about Project Millipedes purpose and what we are aiming at",
  "author": "Markus Gritsch",
  "license": "MIT",
  "keywords": [
    "story",
    "description"
  ],
  "homepage": "https://millipede.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-millipede/millipede-docs.git"
  },
  "bugs": {
    "url": "https://github.com/project-millipede/millipede-docs/issues"
  },
  "scripts": {
    "clean": "rm -rf dist/ .next/",
    "next:dev": "next",
    "next:build": "next build",
    "next:start": "next start",
    "app:build": "tsc",
    "setup:yarn:base": "yarn set version berry && yarn plugin import workspace-tools",
    "setup:yarn:ide": "yarn dlx @yarnpkg/pnpify --sdk vscode",
    "setup:yarn": "yarn setup:yarn:base && yarn setup:yarn:ide",
    "switch:yarn:linker:prod": "yarn config set nodeLinker node-modules && yarn",
    "switch:yarn:linker:dev": "yarn config set nodeLinker pnp && yarn",
    "dev": "NODE_ENV=development && yarn workspaces foreach -pt run clean && yarn workspaces foreach -pt run compile && yarn run app:build && yarn next:dev",
    "start": "NODE_ENV=production && yarn workspaces foreach -pt run clean && yarn workspaces foreach -pt run compile && yarn run app:build && yarn next:build && yarn next:start",
    "vercel-build": "NODE_ENV=production && yarn workspaces foreach -pt run clean && yarn workspaces foreach -pt run compile && yarn run app:build && yarn run next:build",
    "test": "NODE_ENV=development jest --colors --watch",
    "lint": "yarn eslint './docs/src/**/*' './src/**/*' './pages/**/*' './packages/**/src/**/*'",
    "format:mdx": "prettier --parser mdx --print-width 80 --prose-wrap always --single-quote true --write \"docs/src/pages/**/*.mdx\"",
    "format:md": "prettier --parser markdown --print-width 80 --prose-wrap always --single-quote true --write \"docs/src/pages/**/*.md\"",
    "format:json": "prettier --parser json --print-width 80 --use-tabs --write \"locales/de/pages/rethink-security/attackVectors/comparison/content.json\"",
    "sitemap": "node sitemap-generator"
  },
  "main": "index.js",
  "dependencies": {
    "@app/analytics": "workspace:packages/analytics",
    "@app/components": "workspace:packages/components",
    "@app/houx": "workspace:packages/houx",
    "@app/layout": "workspace:packages/layout",
    "@app/mdx-loader": "workspace:packages/mdx-loader",
    "@app/types": "workspace:packages/types",
    "@app/webpack-build": "workspace:build/webpack-build",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@huse/effect-ref": "^1.0.3",
    "@huse/merged-ref": "^1.1.2",
    "@material-ui/core": "^5.0.0-alpha.17",
    "@material-ui/icons": "^5.0.0-alpha.15",
    "@material-ui/lab": "^5.0.0-alpha.17",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "@plusnew/simulate-dom-events": "^0.1.5",
    "@rest-hooks/normalizr": "^5.0.6",
    "@vangware/window-open-promise": "^4.0.8",
    "ahooks": "^2.9.1",
    "chance": "^1.1.7",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.16.1",
    "draft-js": "^0.11.7",
    "element-resize-detector": "^1.2.1",
    "factory.ts": "^0.5.1",
    "framer-motion": "^2.9.4",
    "gray-matter": "^4.0.2",
    "group-array": "^1.0.0",
    "immer": "^8.0.0",
    "jss": "^10.5.0",
    "lodash": "^4.17.20",
    "mdast-util-toc": "^5.1.0",
    "next": "^10.0.3",
    "next-translate": "1.0.0-experimental.4",
    "next-transpile-modules": "^5.0.0",
    "normalizr": "^3.6.1",
    "phaker": "^6.0.1",
    "process": "^0.11.10",
    "react": "17.0.1",
    "react-compound-timer": "^1.2.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "17.0.1",
    "react-hyphen": "^1.4.0",
    "react-intersection-observer": "^8.31.0",
    "react-swipeable-views": "0.13.9",
    "react-swipeable-views-utils": "0.14.0-alpha.0",
    "reading-time-estimator": "1.2.0",
    "recoil": "^0.1.2",
    "remark-parse": "^9.0.0",
    "remark-react": "^8.0.0",
    "remark-slug": "^6.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "typesafe-actions": "^5.1.0",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3",
    "use-analytics": "^0.0.5",
    "use-resize-observer": "^7.0.0",
    "uuid": "^8.3.1",
    "vfile": "^4.2.0",
    "victory": "^35.4.0",
    "windups": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/draft-js": "^0.10.44",
    "@types/element-resize-detector": "^1.1.2",
    "@types/eslint": "^7.2.5",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@types/unist": "^2.0.3",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "nextjs-sitemap-generator": "^1.1.3",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-merge": "^5.4.0"
  },
  "resolutions": {
    "webpack": "^5.9.0",
    "styled-components": "^5.2.1"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./.next/",
      "./dist/",
      "./yarn/",
      "./node_modules/"
    ]
  }
}
